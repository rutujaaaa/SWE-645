<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
    <managed-bean>
<managed-bean-name>surveyBean</managed-bean-name>
<managed-bean-class>managedBean.Student</managed-bean-class>
<managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<navigation-rule>
	<from-view-id>/start-index.xhtml</from-view-id>
	  <navigation-case>
		  <from-outcome>SimpleAcknowledgement</from-outcome>
		 <to-view-id>/result-SimpleAcknowledgement.xhtml</to-view-id>
	 </navigation-case>
	 
	 <navigation-case>
	 	<from-outcome>WinnerAcknowledgement</from-outcome>
	 	<to-view-id>/result-WinnerAcknoeledgement.xhtml</to-view-id>
	 	</navigation-case>
</navigation-rule>
</faces-config>
